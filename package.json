{
    "name": "messed-up-timer",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://salik1992.github.io/messed-up-timer",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "start": "npm start -w @messed-up-timer/rw",
        "build": "npm run build -w @messed-up-timer/rw",
        "test": "npm run test -w",
        "eject": "npm run eject -w @messed-up-timer/rw",
        "predeploy": "npm run build -w @messed-up-timer/rw",
        "deploy": "gh-pages -d packages/rw/build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "gh-pages": "^6.1.1"
    }
}
